{
    "version": 2,
    "builds": [
        {
            "src": "shoestore/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "shoestore/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_ENV": "production",
        "PYTHON_VERSION": "3.9"
    }
}
