{
    "version": 2,
    "builds": [
        {
            "src": "shoestore/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "installCommand": "pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "shoestore/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "shoestore.settings",
        "DJANGO_ENV": "production",
        "PYTHONPATH": "/var/task"
    }
}
